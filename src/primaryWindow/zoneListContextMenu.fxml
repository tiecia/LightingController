<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>

<ContextMenu xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <items>
        <MenuItem mnemonicParsing="false" text="Add Zone" onAction="#addZone"/>
        <MenuItem mnemonicParsing="false" text="Edit Zone" onAction="#editZone" fx:id="editZone"/>
        <MenuItem mnemonicParsing="false" text="Remove Zone" onAction="#removeZone" fx:id="removeZone"/>
    </items>
</ContextMenu>
